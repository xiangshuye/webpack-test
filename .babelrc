{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "not ie <= 8"
                    ]
                },
                "useBuiltIns": "usage"
            }
        ]
    ],
    "plugins": [
        "transform-vue-jsx",
        "transform-runtime",
        "transform-object-rest-spread"
        ,[
            "import",
            {
                "libraryName": "iview",
                "libraryDirectory": "src/components"
            }
        ]
    ]
}
